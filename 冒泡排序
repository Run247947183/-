#define TableSize 256

char FirstNotRepeatingChar(char* string){
	if (string == NULL){
		return '\0';
	}
	unsigned int hashTable[TableSize];
	for (unsigned int i = 0; i < TableSize; ++i){
		hashTable[i] = 0;
	}
	char* pHashKey = string;
